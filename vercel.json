            "runtime": "nodejs16.x",
            "memory": 1024
        },
        "api/sales-delete.js": {
            "runtime": "nodejs16.x",
            "memory": 1024
        },
        "api/sales-put.js": {
            "runtime": "nodejs16.x",
            "memory": 1024
        }
    }
}